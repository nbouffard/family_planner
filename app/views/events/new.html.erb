<h1>hello</h1>
<%= simple_form_for [@user, @member, @event] do |f| %>
  <%= f.input :title %>
  <%= f.input :time %>
  <%= f.input :date %>
  <%= f.input :address %>
  <%= f.input :vaccination %>
  <%= f.input :flea_treatment %>
  <%= f.input :worm_treatment %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit %>
<% end %>
